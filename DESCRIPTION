    'add_characters.R'
    'add_meta.R'
    'add_trees.R'
    'character_classes.R'
    'concat_nexml.R'
    'extend_phylo.R'
    'get_characters.R'
    'get_node_maps.R'
    'isEmpty.R'
    'meta.R'
    'name_by_id.R'
    'nexml_id.R'
    'nexml_phylo_coercions.R'
    'nexml_read.R'
    'nexml_validate.R'
    'nexml_write.R'
    'rdfa_extract.R'
    'taxize.R'
    'toPhylo.R'
